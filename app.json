{
  "expo": {
    "scheme": "guval-portfolio-v6",
    "name": "guval-gh.github.io",
    "slug": "guval-gh.github.io",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
